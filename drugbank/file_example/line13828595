    <articles/>
    <textbooks/>
    <links/>
  </general-references>
  <synthesis-reference/>
  <indication>Used to treat constipation or to clean the bowel before a colonoscopy.</indication>
  <pharmacodynamics/>
  <mechanism-of-action/>
