<drug type="biotech" created="2016-11-18" updated="2017-03-14">
  <drugbank-id primary="true">DB13150</drugbank-id>
  <name>Coagulation factor VII human</name>
  <description>Coagulation factor VII is human serine protease type enzyme that is involved in the extrinsic coagulation cascade which results in blood clotting.</description>
  <cas-number/>
  <unii>4156XVB4QD</unii>
  <state>solid</state>
  <groups>
    <group>approved</group>
  </groups>
