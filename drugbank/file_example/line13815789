  <drugbank-id primary="true">DB09396</drugbank-id>
  <name>Propoxyphene napsylate</name>
  <description>Propoxyphene is a centrally acting narcotic analgesic agent.</description>
  <cas-number/>
  <unii>38M219L1OJ</unii>
